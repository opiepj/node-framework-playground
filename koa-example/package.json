{
  "name": "koa-skeleton",
  "description": "a simple, full-featured koa application made to be forked and modified",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/danneu/koa-skeleton"
  },
  "scripts": {
    "start": "node app.js",
    "start-dev": "nodemon --ext js,html app.js",
    "reset-db": "node ./sql/reset_db",
    "lint": "./node_modules/.bin/standard 'src/**.js' 'sql/**.js' 'test/**.js'",
    "deploy": "git push heroku master"
  },
  "engines": {
    "node": "7.x"
  },
  "dependencies": {
    "autolinker": "^1.4.3",
    "bcryptjs": "2.4.3",
    "better-assert": "1.0.2",
    "debug": "^2.6.4",
    "dotenv": "^4.0.0",
    "interval-cache": "0.0.1",
    "knex": "^0.12.9",
    "koa": "^2.2.0",
    "koa-better-static": "^1.0.5",
    "koa-body": "^1.6.0",
    "koa-bouncer": "^6.0.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-helmet": "^3.1.0",
    "koa-logger": "^2.0.1",
    "koa-nunjucks-render": "^1.2.0",
    "koa-router": "^7.1.1",
    "pg": "^6.1.5",
    "pg-extra": "0.0.13",
    "recaptcha-validator": "^1.0.11",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "standard": "^10.0.2"
  },
  "license": "MIT"
}
